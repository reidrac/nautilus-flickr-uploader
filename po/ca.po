msgstr "<b>Privadesa</b>"

#: UI/Upload.glade.h:4
msgid "Change"
msgstr "Canvia"

#: UI/Upload.glade.h:5
msgid "Family"
msgstr "Família"

#: UI/Upload.glade.h:6
msgid "Friends"
msgstr "Amics"

#: UI/Upload.glade.h:7
msgid "Public"
msgstr "Públic"

#: UI/Upload.glade.h:8
msgid "Put your tags separated by spaces"
msgstr "Posa les teues etiquetes separades per espais"

#: UI/Upload.glade.h:9
msgid "Resize:"
msgstr "Redimensiona:"

#: UI/Upload.glade.h:10
msgid "Tags:"
msgstr "Etiquetes:"

